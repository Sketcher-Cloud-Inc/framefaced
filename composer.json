{
    "name": "sketcher-cloud-inc/framefaced",
    "description": "Application Programming Interface - Framework",
    "authors": [
        {
            "name": "Simon Malpel",
            "email": "simon.malpel@orange.fr"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "System\\": "src/System/Controllers/",
            "System\\Schematics\\": "src/System/Schematics/",
            "System\\Providers\\": "src/Providers/",
            "Services\\": "src/Services/",
            "App\\": "src/App/"
        }
    },
    "require": {
        "symfony/uid": "^5.3",
        "symfony/dotenv": "^5.3"
    },
    "require-dev": {
        "symfony/var-dumper": "^5.3",
        "alecrabbit/php-console-spinner": "^0.54.0",
        "fakerphp/faker": "^1.15"
    }
}
